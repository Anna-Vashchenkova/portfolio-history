<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">
    <changeSet id="2024-05-29-PH-1-01" author="Anna Vashchenkova">
        <createTable tableName="operations">
            <column name="id" type="uuid"/>
            <column name="creatTime" type="datetime"/>
            <column name="accountId" type="uuid"/>
            <column name="assetCode" type="varchar(10)"/>
            <column name="operationType" type="varchar(20)"/>
            <column name="count" type="number"/>
            <column name="unitPrice" type="number"/>
        </createTable>
    </changeSet>


</databaseChangeLog>